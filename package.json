  "devDependencies": {
    "chai": "^4.2.0",
    "coffeescript": "^2.4.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
